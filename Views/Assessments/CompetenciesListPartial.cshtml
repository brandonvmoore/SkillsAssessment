@model List<CompetencyModel>

@for (int i = 0; i < Model.Count; i++)
{
    var s = Model[i];
    <li class="list-group-item py-lg-1@(i == 0 ? " active" : "")" data-id="@s.Id" data-question="@s.Question">
        @s.Title
        <a href="javascript:void(0);"><i class="eye-toggle far fa-eye float-right"></i></a>
    </li>
}
